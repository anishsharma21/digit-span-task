{
    "version": 2,
    "builds": [
      { "src": "server/server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/signup", "dest": "server/server.js" },
      { "src": "/signup/(.*)", "dest": "server/server.js" },
      { "src": "/admin", "dest": "server/server.js" },
      { "src": "/api/(.*)", "dest": "server/server.js" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }